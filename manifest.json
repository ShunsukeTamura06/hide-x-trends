{
    "manifest_version": 3,
    "name": "__MSG_extensionName__",
    "version": "1.0",
    "description": "__MSG_extensionDescription__",
    "default_locale": "en",
    "host_permissions": [
        "*://x.com/*",
        "*://*.x.com/*"
    ],
    "icons": {
        "16": "icon/icon16.png",
        "32": "icon/icon32.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://x.com/*",
                "*://*.x.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}